<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:PRJS.ViewModels"
             x:Class="PRJS.Views.GridDataPage"
             Title="GridDataPage">
    <ContentPage.BindingContext>
        <vm:GridViewModel/>
    </ContentPage.BindingContext>
    <ScrollView>
    <StackLayout>
    <Frame>
            <StackLayout>
                <Label  HorizontalOptions="CenterAndExpand" FontSize="20" FontAttributes="Bold" Text="Data Invoice Sell"/>
                <telerik:RadDataGrid  ItemsSource="{Binding ListInvoiceSell}" AutoGenerateColumns="True">
                </telerik:RadDataGrid>
            </StackLayout>

    </Frame>
        <StackLayout>
                <Label  HorizontalOptions="CenterAndExpand" FontSize="20" FontAttributes="Bold" Text="Data Invoice Sell Unit"/>
            <telerik:RadDataGrid  ItemsSource="{Binding  ListInvoiceSellUnit}" AutoGenerateColumns="True">
            </telerik:RadDataGrid>
        </StackLayout>
        <Frame>
        
    </Frame>
    </StackLayout>
    </ScrollView>
</ContentPage>