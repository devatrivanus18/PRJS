<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:PRJS.ViewModels"
             x:Class="PRJS.Views.GridDataPage"
             Title="GridDataPage">
    <ContentPage.BindingContext>
        <vm:GridViewModel/>
    </ContentPage.BindingContext>
    <telerik:RadDataGrid x:Name="dataGrid" ItemsSource="{Binding ListInvoiceSell}" AutoGenerateColumns="False">
        <telerik:RadDataGrid.Columns>
            <telerik:DataGridTextColumn  CanUserEdit="True" PropertyName="eName" SizeMode="Auto"/>
            <telerik:DataGridNumericalColumn PropertyName="invoiceVATID" SizeMode="Auto"/>
        </telerik:RadDataGrid.Columns>
    </telerik:RadDataGrid>
</ContentPage>